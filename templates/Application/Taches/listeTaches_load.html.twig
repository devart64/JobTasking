{% for  Piece in listePieces %}
	
	<h3 >{{ Piece.intitule }}</h3 >
	<div class="box" >
		<div class="box-body" >
			{% for taches in Piece.getTache() %}
				<div >
					<a href="{{ path('tache', {'id':taches.id}) }}" ><i class="mdi {{ taches.icon }}" ></i > {{ taches.intitule }}
					</a >
				</div >
			{% endfor %}
		</div >
	</div >


{% endfor %}
